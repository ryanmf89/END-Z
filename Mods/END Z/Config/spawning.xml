<configs>

<append xpath="//biome[@name='wasteland']">
	<spawn maxcount="1" respawndelay="0.3" time="Day"	entitygroup="BossWastelandZombies" notags="commercial,industrial,downtown" />
	<spawn maxcount="2" respawndelay="0.001" time="Night" entitygroup="BossWastelandZombies" notags="commercial,industrial,downtown" />

	<spawn maxcount="4" respawndelay="0.033" time="Day" entitygroup="BossWastelandZombies" tags="commercial,industrial" notags="downtown" />
	<spawn maxcount="4" respawndelay="0.002" time="Night" entitygroup="BossWastelandZombies" tags="commercial,industrial" notags="downtown" />

	<spawn maxcount="4" respawndelay="0.0167" time="Day" entitygroup="BossWastelandZombies" tags="downtown" />
	<spawn maxcount="4" respawndelay="0.002" time="Night" entitygroup="BossWastelandZombies" tags="downtown" />

	<spawn maxcount="1" respawndelay="0.3" time="Any" entitygroup="BossWastelandZombies" spawnDeadChance="0" />
</append>

