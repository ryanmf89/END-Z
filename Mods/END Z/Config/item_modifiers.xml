<!--HAZMAT MODS FOR ARMOR-->
<append xpath="/item_modifiers">
<item_modifier name="modRadiationReadyhatLL" installable_tags="head" modifier_tags="resistance" blocked_tags="noMods" type="attachment">
	<property name="CustomIcon" value="apparelHazmatMask" />
	<property name="Material" value="MresourceHazmatFiberLL"/>
	<property name="Extends" value="modGeneralMaster"/>
	<property name="CreativeMode" value="Player"/>
	<effect_group tiered="false">
